@model QuestEngine.ViewModels.StatisticsViewModel

@{
    ViewBag.Title = "Статистика";
}

<h2>Статистика</h2>


<table class="table">
    <tr>
        <td>
            
        </td>
        @for (int i = 1; i <= 9 ; i++)
        {
            <td>
                @i
            </td>
        }
        <td>Финиш</td>
    </tr>
@foreach (var item in Model.TeamSatistics)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TeamName)
        </td>
        @foreach (var riddle in item.Riddles)
        {
        <td>
            @Html.DisplayFor(modelItem => riddle.StatTime)
        </td>
        }
    </tr>
}

</table>
